<model name="ad:autodealmodel" xmlns="http://www.alfresco.org/model/dictionary/1.0">
    <description>Auto Deal Model</description>
    <author>MSI</author>
    <version>1.0</version>

    <imports>
        <!-- Import Alfresco Dictionary Definitions -->
        <import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
        <!-- Import Alfresco Content Domain Model Definitions -->
        <import uri="http://www.alfresco.org/model/content/1.0" prefix="cm"/>
    </imports>

    <namespaces>
        <namespace uri="http://www.someco.com/automodel/1.0" prefix="ad"/>
    </namespaces>

    <types>
        <type name="ad:autoDealOperation">
            <title>Auto Deal Operation</title>
            <parent>cm:content</parent>
            <properties>
                <property name="ad:carMake">
                    <type>d:text</type>
                </property>
                <property name="ad:carModel">
                    <type>d:text</type>
                </property>
                <property name="ad:cost">
                    <type>d:int</type>
                </property>
                <property name="ad:seller">
                    <type>cm:person</type>
                </property>
                <property name="ad:costomer">
                    <type>cm:person</type>
                </property>
            </properties>
        </type>
    </types>


</model>